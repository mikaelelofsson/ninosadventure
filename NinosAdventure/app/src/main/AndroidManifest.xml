<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mikaelelofsson.ninosadventure">




    <application
        android:allowBackup="true"
        android:icon="@drawable/nino"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen"
        android:hardwareAccelerated="false">
        <activity
            android:name="com.example.mikaelelofsson.ninosadventure.MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.mikaelelofsson.ninosadventure.TownActivity"
            android:screenOrientation="landscape"/>
        <activity android:name="com.example.mikaelelofsson.ninosadventure.BusTravelActivity"
            android:screenOrientation="landscape" />
        <activity android:name="com.example.mikaelelofsson.ninosadventure.ConstructionActivity"
            android:screenOrientation="landscape"/>
        <activity android:name="com.example.mikaelelofsson.ninosadventure.TrainStationActivity"
            android:screenOrientation="landscape" />
        <activity android:name="com.example.mikaelelofsson.ninosadventure.CountrySideActivity"
            android:screenOrientation="landscape" />
        <activity android:name="com.example.mikaelelofsson.ninosadventure.OceanActivity"
            android:screenOrientation="landscape"/>
        <activity android:name="com.example.mikaelelofsson.ninosadventure.AirportActivity"
            android:screenOrientation="landscape"/>
        <activity android:name="com.example.mikaelelofsson.ninosadventure.LoadGame"
            android:screenOrientation="landscape"/>
        <activity android:name="com.example.mikaelelofsson.ninosadventure.SaveGame"
            android:screenOrientation="landscape" />

    </application>

</manifest>